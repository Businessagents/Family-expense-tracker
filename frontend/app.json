{
  "expo": {
    "name": "Family Expense Tracker",
    "slug": "family-expense-tracker-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "familyexpensetracker",
    "userInterfaceStyle": "automatic",
    "description": "A collaborative family expense tracking app with secure authentication and real-time expense management",
    "privacy": "https://raw.githubusercontent.com/Businessagents/Family-expense-tracker/main/docs/privacy-policy.html",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adhia.familyexpensetracker"
    },
    "android": {
      "package": "com.adhia.familyexpensetracker",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0B1F2A"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "USE_BIOMETRIC",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID for authentication"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-image.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0B1F2A"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "businessagents",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2480b33a-7288-491a-9f24-57a95906019a"
      }
    }
  }
}
